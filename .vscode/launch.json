  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Inpatients (principal) Run Model",
      "type": "python",
      "request": "launch",
      "program": "run_model.py",
      "args": [
        "test/queue/synthetic.json",
        "--debug",
        "--type=ip"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Outpatients (principal) Run Model",
      "type": "python",
      "request": "launch",
      "program": "run_model.py",
      "args": [
        "test/queue/synthetic.json",
        "--debug",
        "--type=op"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Python: A&E (principal) Run Model",
      "type": "python",
      "request": "launch",
      "program": "run_model.py",
      "args": [
        "test/queue/synthetic.json",
        "--debug",
        "--type=aae"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Run All Principal Projections (local save)",
      "type": "python",
      "request": "launch",
      "program": "run_model.py",
      "args": [
        "test/queue/synthetic.json",
        "-b=1",
        "-c=1"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Run All Models (parallel/local save)",
      "type": "python",
      "request": "launch",
      "program": "run_model.py",
      "args": [
        "test/queue/synthetic.json",
        "--model_runs=257",
        "-b=8"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Run All Models (parallel/cosmos save)",
      "type": "python",
      "request": "launch",
      "program": "run_model.py",
      "args": [
        "test/queue/synthetic.json",
        "--save-type=cosmos",
        "--results_path=nhp_results",
        "--model_runs=257",
        "-b=16"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Combine Results",
      "type": "python",
      "request": "launch",
      "program": "combine_results.py",
      "args": [
        "run_results",
        "results",
        "synthetic",
        "test",
        "20220101_000000"
      ]
    },
    {
      "name": "Batch: Run Queue",
      "type": "python",
      "request": "launch",
      "program": "run_batch.py",
      "args": [
        "run_tasks",
        "64",
        "test/queue"
      ]
    }
  ]
}